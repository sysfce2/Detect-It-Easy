meta("format", "PlayStation executable (PS1)");

function detect() {
    if (Binary.compare("'PS-X EXE'")) {
        var regionString = Binary.getString(76, 48).trim();

        if (regionString) {
            if (X.isVerbose()) sOptions = "Region: " + regionString;
            bDetected = true;
        }
    }
    
    return result();
}