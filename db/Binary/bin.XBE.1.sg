// https://xboxdevwiki.net/Xbe
meta("format", "Xbox Executable (.XBE)");

function detect() {
    if (Binary.compare("'XBEH'000000000000000000000000")) {
        sOptions = "unsigned";
        bDetected = true;
    }

    return result();
}