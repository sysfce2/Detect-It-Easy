meta("format", "BOU");

function detect() {
    var appli = Binary.getString(0, 20).trim();

    if (/^[CLcl]\d{2}([Cc]alcul|[Ll]ecture) \d ans/.test(appli)) {
        if (X.isVerbose()) sOptions = appli;
        bDetected = true;
    }

    return result();
}