// https://graphicsgale.com/
meta("format", "Gale (.GAL)");

function detect() {
    if (Binary.compare("'Gale'")) {
        var versionNumber = Binary.getString(4, 3).trim();

        if (/^[A-Z0-9]{3}$/.test(versionNumber)) {
            sVersion = versionNumber;
            bDetected = true;
        }
    }

    return result();
}