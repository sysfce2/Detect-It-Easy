// https://en.wikipedia.org/wiki/MATLAB
meta("format", "MatLab");

function detect() {
    if (Binary.compare("'MATLAB'")) {
        var versionNumber = Binary.getString(7, 3).trim();

        if (/^[0-9/.]{3}$/.test(versionNumber)) {
            if (X.isVerbose()) sVersion = versionNumber;
            bDetected = true;
        }
    }

    return result();
}