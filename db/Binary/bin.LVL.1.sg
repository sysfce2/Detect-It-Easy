// https://www.mobygames.com/game/138556/the-energy-thieves/ Used by Adiboo & The Energy Thieves (2004)
meta("format", "LVL");

function detect() {
    if (Binary.compare("1C070000....0000")) {
        var worldrender = Binary.getString(16, 13).trim();

        if (/^[A-Za-z]{13}/.test(worldrender)) {
            sOptions = worldrender;
            bDetected = true;
        }
    }

    return result();
}