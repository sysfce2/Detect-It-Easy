// https://en.wikipedia.org/wiki/Public-key_cryptography
meta("format", "SSH Public Key");

function detect() {
    if (Binary.compare("'ssh-ed25519'20")) {
        sOptions = "ed25519";
        bDetected = true;
    } else if (Binary.compare("'ssh-rsa'20")) {
        sOptions = "RSA";
        bDetected = true;
    }

    return result();
}