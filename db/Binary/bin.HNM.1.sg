// https://en.wikipedia.org/wiki/Woody_Woodpecker:_Escape_from_Buzz_Buzzard_Park
meta("format", "HNM");

function detect() {
    if (Binary.compare("'HNM6'0000..108002")) {
        bDetected = true;
    }

    return result();
}