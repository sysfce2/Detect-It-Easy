meta("format", "DTA");

function detect() {
    if (Binary.compare("03000000....08")) {
        var model = Binary.getString(7, 8).trim();

        if (/^[a-z0-9]{8}/.test(model)) {
            if (X.isVerbose()) sOptions = model;
            bDetected = true;
        }
    }

    return result();
}