meta("library", "Electron package");

function detect() {
    var cpadInfoSection = PE.section["CPADinfo"];

    if (cpadInfoSection &&
        PE.isSectionNamePresent(".00cfg")) {

        bDetected = PE.findString(cpadInfoSection.FileOffset, cpadInfoSection.FileSize, "daPC");
    }

    return result();
}